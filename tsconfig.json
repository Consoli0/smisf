{
  "compilerOptions": {
    "outDir": "dist/",
    "alwaysStrict": true,
    //"noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "declaration": true,
    "target": "ES6",
    //"module": "ES6",
    "moduleResolution": "node",
    "lib": ["ES2020.String"]
  },
  "include": ["lib/**/*"],
  "exclude": ["node_modules", "dist"]
}
